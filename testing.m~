
files = ["jake.wav" "tammany.wav" "lindsay.wav"];
numfiles = size(files,2);


%Calculate MFCCs
mfccs = cell(1,numfiles);
for i = 1:numfiles
    filename = files(1,i);
    
    mffcs_for_filename; %run on file 'filename'
    
    mfccs(1,i) = {MFCCs};
 
end
close all;


